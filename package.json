{
  "name": "flairverse",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node ./server",
    "start": "npm run dev",
    "storybook": "start-storybook -p 9997 -s public",
    "build": "next build",
    "build-storybook": "build-storybook",
    "lint": "next lint",
    "prepare": "husky install",
    "format": "npx prettier --write .",
    "cy:open": "npx cypress open",
    "cy:run": "npx cypress run",
    "cy:chrome": "cypress run --browser chrome --reporter json",
    "cy:firefox": "cypress run --browser firefox --reporter json",
    "cy:edge": "cypress run --browser edge --reporter json",
    "cy:clear-reps:wins": "del .\\reporters\\reports\\junit\\*",
    "cy:clear-reps:unix": "rm -r reporters/reports/junit/*",
    "jest:run": "jest --silent=true --json --outputFile=./reporters/jest-reporter.json && yarn beautify:jest-result",
    "jest:watch": "jest --watch --silent=false --json --outputFile=./reporters/jest-reporter.json && yarn beautify:jest-result",
    "jest:ci": "jest --ci --json --outputFile=./reporters/jest-reporter.json && yarn beautify:jest-result",
    "beautify:jest-result": "node ./scripts/beautify-json-file.js --path=./reporters/jest-reporter.json --indent=2",
    "update:jest-result-path": "node ./scripts/update-jest-result-path.js",
    "update:story-book-port": "node ./scripts/update-storybook-port.js"
  },
  "dependencies": {
    "@giphy/js-fetch-api": "^4.1.2",
    "@giphy/react-components": "^5.6.1",
    "@svgr/webpack": "^6.2.1",
    "antd": "^4.21.4",
    "axios": "^0.26.0",
    "dexie": "^3.2.2",
    "dexie-react-hooks": "^1.1.1",
    "express": "^4.17.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "next": "12.1.0",
    "next-i18next": "7.0.1",
    "next-plugin-antd-less": "^1.8.0",
    "next-themes": "^0.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.3.1",
    "react-query": "^3.34.16",
    "react-sticky-box": "^1.0.1",
    "recoil": "^0.6.1",
    "styled-components": "^5.3.3",
    "ts-mixer": "^6.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/plugin-proposal-decorators": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.6",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-docs": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-jest": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/addon-storysource": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "17.0.21",
    "@types/react": "17.0.39",
    "@types/styled-components": "^5.1.23",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.5",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-styled-components": "^2.0.6",
    "cypress": "^10.3.0",
    "cypress-multi-reporters": "^1.5.0",
    "eslint": "^8.21.0",
    "eslint-config-next": "^12.2.4",
    "eslint-plugin-testing-library": "^5.6.0",
    "http-proxy-middleware": "^2.0.3",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "junit-merge": "^2.0.0",
    "mocha-junit-reporter": "^2.0.2",
    "mochawesome": "^7.1.0",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.1.0",
    "msw": "^0.44.2",
    "msw-storybook-addon": "^1.6.3",
    "sass": "^1.49.9",
    "storybook-addon-performance": "^0.16.1",
    "storybook-dark-mode": "^1.1.0",
    "typescript": "^4.7.3"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
